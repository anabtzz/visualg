algoritmo "exercicio2"
// Função :
// Autor :
// Data : 17/03/2023
// Seção de Declarações 
var
  numtabuada,result: real
  i: inteiro
inicio

i:1
   escreva("Digite o numero da tabuada")
leia(numtabuada)

enquanto(i<+10)faca

      result:=numtabuada*i
      i=i1

    escreva("O resultado é", result)
ate(i>10)

fimalgoritmo
