algoritmo "exercicio2/3"
// Função :
// Autor :
// Data : 17/03/2023
// Seção de Declarações 
var
    numtabuada,result:real
    i:inteiro
inicio
i:=1
   escreva("Digite o numero da tabuada:")
leia(numtabuada)

para i  de 1 ate 10 passo 1 faca

      result:=numtabuada*i
      i:=i+1

    escreval("O resultado é:", result)
    
    fimpara
    
fimalgoritmo
